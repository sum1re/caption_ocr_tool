{
  "openapi": "3.0.1",
  "info": {
    "title": "cocr api",
    "description": "api for caption_ocr_tool",
    "version": "1.0.0"
  },
  "tags": [],
  "paths": {},
  "components": {
    "schemas": {
      "RestVo": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean"
          },
          "errors": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/RestErrorVo"
            }
          },
          "result": {
            "oneOf": [
              {
                "type": "object",
                "properties": {},
                "additionalProperties": false,
                "nullable": true
              },
              {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {}
                }
              }
            ]
          },
          "resultInfo": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/RestPageVo"
              },
              {
                "nullable": true
              }
            ]
          }
        },
        "required": [
          "success",
          "errors",
          "result"
        ]
      },
      "RestPageVo": {
        "type": "object",
        "properties": {
          "count": {
            "type": "number",
            "description": "element count"
          },
          "size": {
            "type": "number",
            "description": "element count per page"
          },
          "totalPage": {
            "type": "number",
            "description": "total page"
          },
          "totalElements": {
            "type": "number",
            "description": "total elements"
          }
        },
        "required": [
          "count",
          "size",
          "totalPage",
          "totalElements"
        ]
      },
      "RestErrorVo": {
        "type": "object",
        "properties": {
          "code": {
            "type": "integer",
            "description": "error code, 1xxx or 10xxxx"
          },
          "message": {
            "type": "string",
            "description": "error message"
          }
        },
        "required": [
          "code",
          "message"
        ]
      }
    }
  },
  "servers": []
}